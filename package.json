{
  "name": "kite",
  "version": "0.2.5",
  "description": "General purpose CLI utility for deploying Docker-based infastructure",
  "bin": {
    "kite": "index.js"
  },
  "main": "index.js",
  "dependencies": {
    "dockerode": "^2.2.8",
    "js-yaml": "^3.4.6",
    "shelljs": "^0.5.3",
    "yargs": "^3.31.0"
  },
  "devDependencies": {
    "http-server": "^0.8.5",
    "jasmine-node": "^1.14.5",
    "jsdoc": "^3.4.0",
    "jsh-cli": "^0.1.3",
    "standard": "^5.4.1"
  },
  "scripts": {
    "clean": "jsh rm -r out",
    "docs:build": "jsdoc . lib",
    "docs:deploy": "jsdoc . -d /tmp/__temp_static_content",
    "docs:serve": "hs out",
    "docs": "npm run docs:build -s && npm run docs:serve -s",
    "test:style": "standard",
    "test:code": "jasmine-node spec",
    "test": "npm run test:style -s && npm run test:code -s"
  },
  "standard": {
    "ignore": [
      "out",
      "spec"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jcpst/kite.git"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jcpst/kite/issues"
  },
  "homepage": "https://github.com/jcpst/kite#readme"
}
